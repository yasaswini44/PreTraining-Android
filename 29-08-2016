


interface Student{
 String program;
 int year;
 double fee;
	public Student(String name,String address,String program,int year,double fee){
		void function();
		String getProgram();
		void setProgram(String program);
		int getYear();
		void setYear(int year);
		double getFee();
		void setFee(double fee);
		String toString();
}

interface Staff{
 String school;
 double pay;
	public Staff(String name,String school,double pay){
		String getSchool();
		void setSchool(String school);
		double getPay();
		void setPay(double pay);
		String toString();
}
			
public abstract class Person implements Student,Staff{
 String name;
 String address;
